#include<stdio.h>
#include<stdlib.h>

int main()
{
    char p[100];
    char p1[100];
    scanf("%s",&p);
    int i=0;
    int j=0;
    while(p[i]!='\0')
    {
        if(p[i]=='a'||p[i]=='e'||p[i]=='i'||p[i]=='o'||p[i]=='u')
        {
            p1[j]=p[i];
            j++;
            i++;
        }
        else
            i++;
    }
    p1[j]='\0';
    printf("%s",p1);


    return 0;
}
